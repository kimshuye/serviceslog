{
  "rules": {
    ".read": true,
    ".write": "auth != null",
    "users": {
      ".write": "auth != null",
      "username": {
        ".validate": "!root.child('usernames').child(newData.val()).exists()"
      }
    },
    "usernames": {
      ".write": "auth != null"
    },
    "notes": {
      ".write": "auth != null"
    }
  }
}
